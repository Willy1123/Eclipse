/*
 * Esta es la clase principal, que llama a las demás clases
 * para poder construir el robot
 */
import acm.program.*;
import acm.graphics.*;


public class ContruirRobot extends GraphicsProgram{
	
	public static int ANCHO_PANTALLA = 800;
	public static int ALTO_PANTALLA = 600;
	
	public void init() {
		setSize(ANCHO_PANTALLA, ALTO_PANTALLA);
	}
	
	public void run() {
		CaraRobot c = new CaraRobot();
		add(c);
	}

}
