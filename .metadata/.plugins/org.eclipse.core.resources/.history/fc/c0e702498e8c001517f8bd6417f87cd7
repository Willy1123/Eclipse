import acm.program.*;
public class EjerciciosBasicos extends ConsoleProgram{
	
	public boolean fiestaArdilla(int numeroBellotas, boolean festivo) {
		if(numeroBellotas < 30 && numeroBellotas>60 || !festivo) {
			return true;
		}
		return false;
	}
	
	public void run() {
		println(fiestaArdilla(30, false));
	}
}
